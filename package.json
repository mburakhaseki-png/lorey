{
  "name": "lorey",
  "version": "1.0.0",
  "description": "Turn any boring lesson into an interactive, fun, and story-based learning experience",
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "dev:next": "next dev -p 3000",
    "dev:server": "nodemon server/index.js",
    "build": "next build",
    "start": "concurrently \"next start\" \"node server/index.js\"",
    "lint": "next lint"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.0",
    "framer-motion": "^11.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "konva": "^9.2.0",
    "multer": "^1.4.5-lts.1",
    "next": "^14.2.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.0",
    "react-konva": "^18.2.10",
    "youtube-transcript": "^1.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/multer": "^1.4.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0"
  }
}
